
# Einstellung: SE param1, param2, param3, param4, param5, param6
# 	param1 - Wasseruhr Nr
# 	param2 - MinSoll
# 	param3 - MaxSoll
# 	param4 - Qn 1, 2, 3, 6 und 10 ~ 1.5, 2.5, 1.5N, 10N und 10
# 	param5 - Kubikmeter
# 	param6 - Liter, 1000 liter => 1 Kubikmeter
 
record(longout, "$(CARD):$(CHAN):min_set")
{
    field(DESC, "Set minimal water flow rate (Liter/hour)")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto min_set($(CARD),$(CHAN)) $(PORT)")
    field(DRVH, "30000")
    field(HOPR, "30000")
    field(EGU,  "l/h")
    field(PRIO, "HIGH")
    info(autosaveFields_pass1, "VAL")
    field(TPRO, "1")
}

record(longout, "$(CARD):$(CHAN):max_set")
{
    field(DESC, "Set maximal water flow rate ")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto max_set($(CARD),$(CHAN)) $(PORT)")
    field(DRVH, "30000")
    field(HOPR, "30000")
    field(EGU,  "l/h")
    field(PRIO, "HIGH")
}

