# CL - clear interlock
record(ao, "CL:set")
{
    field(DESC, "Clear the interlock queue, producing new alarm")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto set:uhr:CL $(PORT)")
    field(DRVH, "1")
    field(HOPR, "1")
    field(LOPR, "1")
}


