record(ao, "uhr:Minsoll:set")
{
    field(DESC, "Set minimal water flow rate (Liter/hour)")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto set:uhr:Minsoll $(PORT)")
    field(PREC, "2")
    field(DRVH, "30000")
    field(HOPR, "30000")
    field(LOPR, "0")
    field(EGU, "l/h")

}

record(ao, "uhr:Maxsoll:set")
{
    field(DESC, "Set maximal water flow rate ")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto set:uhr:Maxsoll $(PORT)")
    field(PREC, "2")
    field(DRVH, "30000")
    field(HOPR, "30000")
    field(LOPR, "0")
    field(EGU, "l/h")
}

record(ao, "uhr:Qn:set")
{
    field(DESC, "Set Qn value")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto set:uhr:Qn $(PORT)")
    field(DRVH, "10")
    field(HOPR, "10")
    field(LOPR, "1")
    field(EGU, "cubic meter/h")
}

record(ao, "uhr:Kubic:set")
{
    field(DESC, "Set cubic meter L/h")
