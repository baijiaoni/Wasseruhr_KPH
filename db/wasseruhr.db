# CL - clear interlock
record(ao, "$(CARD):$(CHAN):clear_set")
{
    field(DESC, "Clear the interlock queue")
    field(DTYP, "stream")
    field(OUT,  "@wasseruhr.proto clear_set($(CARD),$(CHAN)) $(PORT)")
    field(DRVH, "1")
    field(HOPR, "1")
    field(LOPR, "1")
    field(PRIO, "HIGH")
    field(FLNK, "$(CARD):$(CHAN):trigger-default")
}

